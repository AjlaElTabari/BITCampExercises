package predavanja;

public class LinkedListDouble {
	private Node start;
	
	private class Node {
		private int value;
		private Node next;
		
		public Node(int value) {
			this.value = value;
		}
		
		public int getValue() {
			return value;
		}
		
		public Node getNextNode() {
			return next;
		}
		
		public void setNextNode(Node next) {
			this.next = next;
		}
		
		@Override
		public String toString() {
			if (next == null) {
				return value + ""; 
			}
			
			return value + " " + next.toString();
		}
	}
}
