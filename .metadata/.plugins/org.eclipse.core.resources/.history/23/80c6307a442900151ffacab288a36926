package predavanja;

public class Trip {
	private Place start;
	
	public Trip(Place start) {
		this.start = start;
	}
	
	/**
	 * @return the start
	 */
	public Place getStart() {
		return start;
	}
	
	/**
	 * @param start the start to set
	 */
	public void setStart(Place start) {
		this.start = start;
	}

	public void addPlace(Place newPlace) {
		
		Place last = start;
		
		while (last != null) {
			last = last.getNext();
		}
		
		
		if(start == null && last == null) {
			start = newPlace;
			last = newPlace;
		} else {
			last.setNext(newPlace);
			last = newPlace;
		}
	}
	
	
	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return start.toString();
	}
}
