package vjezbe.zadaca;

public class Main {

	public static void main(String[] args) {
		StringStack stack = new StringStack();
		stack.push("Ajla");
		stack.push("Amra");
		stack.push("Gordan");
		stack.push("Zaid");
		System.out.println(stack.peek());
		System.out.println(stack);
		stack.pop();
		System.out.println(stack.peek());
		System.out.println(stack);
		stack.pop();
		System.out.println(stack.peek());
		System.out.println(stack);
		stack.pop();
		System.out.println(stack.peek());
		System.out.println(stack);
		System.out.println(stack.empty());

	}

}
