package predavanja;

public class Trip {
	/**
	 * @return the from
	 */
	public Place getFrom() {
		return from;
	}

	/**
	 * @param from the from to set
	 */
	public void setFrom(Place from) {
		this.from = from;
	}

	/**
	 * @return the to
	 */
	public Place getTo() {
		return to;
	}

	/**
	 * @param to the to to set
	 */
	public void setTo(Place to) {
		this.to = to;
	}

	private Place from;
	private Place to;
	
	/**
	 * @param from
	 * @param to
	 */
	public Trip(Place from, Place to) {
		super();
		this.from = from;
		this.to = to;
	}
	
	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "Trip from " + from + " to " + to;
	}
}
