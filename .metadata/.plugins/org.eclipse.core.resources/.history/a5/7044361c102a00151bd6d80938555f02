package predavanja;

public class LinkedListInts {
	private Node start;
	
	public void addFront(int value) {
		Node newNode = new Node();
		newNode.value = value;
		newNode.next = start;
		start = newNode;
	}
	
	private boolean isEmpty() {
	return (start == null) ? true : false;
	}
	
	class Node {
		int value;
		Node next;  
	}
}
