		// POP
		long startStackArray = System.currentTimeMillis();
		for (int i = 0; i < 100000; i++) {
			stackArray.pop();
		}
		System.out.println("stackArray push: " + (System.currentTimeMillis() - startStackArray));
		
		long startStackLinkedLists = System.currentTimeMillis();
		for (int i = 0; i < 100000; i++) {
			stackLinkedLists.pop();
		}
		System.out.println("startStackLinkedLists push: " + (System.currentTimeMillis() - startStackLinkedLists));
