package predavanja;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Departure");
		Place departure = new Place(input.nextLine());
		System.out.println("Destination");
		Place destination = new Place(input.nextLine());
		Trip trip = new Trip(departure, destination);

	}

}
